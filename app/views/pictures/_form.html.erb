<%= form_with model: @picture, local: true do |f| %>
<%= f.label :full_name %>
<%= f.text_field :full_name, placeholder: "Nome completo"%>
<br>
<%= f.label :password %>
<%=f.text_field :password, placeholder: "senha"%>
<br>
<%= f.file_field :avatar,  height: 300, width: 400  %>
<br>
<br>
  <%= f.submit 'Salvar' %>
<%end%>
